{
  "manifest_version": 2,
  "name": "bl.ocks.org",
  "version": "1.3.4",
  "short_name": "bl.ocks",
  "description": "View any gist on bl.ocks.org.",
  "permissions": [
    "http://*/*", "https://*/*", "file://*/*"
  ],
  "icons": {"128": "icon-128.png"},
  "content_scripts": [
    {
      "matches": ["https://gist.github.com/*"],
      "js": ["blocks.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": ["*.png"]
}
